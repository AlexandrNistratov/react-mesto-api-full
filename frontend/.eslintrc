{
  "extends": "react-app",
  "ignorePatterns": ["/build/*"],
  "rules": {
    "no-unused-vars": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id", "_url"] }],
    "no-undef": "off",
    "camelcase": "off",
    "import/prefer-default-export": "off",
    "prefer-promise-reject-errors": "off",
    "class-methods-use-this": "off",
    "no-use-before-define": "off",
    "consistent-return": "off",
    "no-console": "off",
    "no-control-regex": 0,
    "no-useless-escape": 0,
    "func-names": [
      "error",
      "never"
    ]
  }
}

